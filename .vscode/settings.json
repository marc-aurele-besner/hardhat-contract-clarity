{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.tslint": true
  },
  "eslint.options": {
    "extensions": [".js", ".md", ".ts"]
  },
  "eslint.validate": [
    "markdown",
    "javascript",
    "typescript",
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "window.zoomLevel": -1,
  "editor.tabSize": 2,
  "editor.formatOnSave": true
}
